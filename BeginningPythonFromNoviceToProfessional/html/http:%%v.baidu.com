!function(){        require.async(['common:static/vendor/jquery/jquery.js', 'common:static/ui/oo/klass.js'], function ($, Base) {

            var $elWrap = $('#showHotWatch');
            var $tabLeft = $elWrap.find('.carousel-ctrl-left .ctrl-left');
            var $tabRight = $elWrap.find('.carousel-ctrl-right .ctrl-right');

            var Carousel = function (opt) {
                var $wrap = $(opt.wrap);
                var $view = $(opt.view);
                var $items = $(opt.items);

                if (!$items.filter(':last').children().length) {
                    $items.filter(':last').remove();
                }

                var $btnLeft = $(opt.btnLeft);
                var $btnRight = $(opt.btnRight);
                var $maskLeft = $wrap.find('.mask-left');
                var $maskRight = $wrap.find('.mask-right');

                var totalWidth;
                var fn = function () {
                    totalWidth = 0;
                    $items.each(function (i) {
                        totalWidth += $items.eq(i).outerWidth(true);
                    });

                    $view.css({
                        position: 'relative',
                        marginLeft: 0,
                        width: totalWidth
                    });

                    if (totalWidth <= $wrap.width()) {
                        $btnLeft.hide();
                        $maskLeft.hide();
                        $btnRight.hide();
                        $maskRight.hide();
                    }
                    else {
                        $btnLeft.hide();
                        $maskLeft.hide();
                        $btnRight.show();
                        $maskRight.show();
                    }
                }
                $(window).on('window:resize', fn);
                fn();

                $btnRight.on('click', function (e) {
                    e.preventDefault();
                    var oldMargin = Math.abs(parseInt($view.css('margin-left'), 10));
                    var marginLeft = Math.min(totalWidth - $wrap.width(), oldMargin + $wrap.width());

                    if (marginLeft === totalWidth - $wrap.width()) {
                        $btnLeft.show();
                        $maskLeft.show();
                        $btnRight.hide();
                        $maskRight.hide();
                    }

                    $view.animate({'margin-left': -marginLeft}, 300);
                });

                $btnLeft.on('click', function (e) {
                    e.preventDefault();
                    var oldMargin = parseInt($view.css('margin-left'), 10);
                    var marginLeft = Math.max(oldMargin - $wrap.width(), 0);

                    if (marginLeft === 0) {
                        $btnLeft.hide();
                        $maskLeft.hide();
                        $btnRight.show();
                        $maskRight.show();
                    }

                    $view.animate({'margin-left': marginLeft}, 300);
                });
            }

            new Carousel({
                wrap: $elWrap,
                view: $elWrap.find('.hot-point-list'),
                items: $elWrap.find('.hot-point-item'),
                btnLeft: $tabLeft,
                btnRight: $tabRight
            });
        });
    }();
